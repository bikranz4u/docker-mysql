{
  "AWSEBDockerrunVersion": "1",
  "Image": {
    "Name": "httpd",
    "Update": "true"
  },
  "Ports": [
    {
      "ContainerPort": "80"
    }
  ],
  "Volumes": [
    {
      "HostDirectory": "/var/app/current",
      "ContainerDirectory": "/usr/local/apache2/htdocs"
    }
  ],
  "Logging": "/var/log/myapp"
}

